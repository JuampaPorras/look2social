<div id="content">
    <!-- #content -->
    <!-- SECTION START -->
    <section class="services-preview white-bg mt20">
        <div class="container">
            <div class="row service-panel style1">
                <!-- .row start -->
                <div class="col-md-4">
                    <!-- col-md-4 start here -->
                    <div class="service-panel-heading">
                        <i class="l-basic-clock color-red"></i>
                        <h4>SOCIAL LISTENING ON TIME</h4>
                    </div>
                    <div class="service-panel-content">
                        <p>Our team works quickly to make sure you listen to what social networks have to tell you on time!</p>
                    </div>
                </div>
                <!-- col-md-4 end here -->
                <div class="col-md-4">
                    <!-- col-md-4 start here -->
                    <div class="service-panel-heading">
                        <i class="l-basic-heart color-red"></i>
                        <h4>We want you to succeed</h4>
                    </div>
                    <div class="service-panel-content">
                        <p>We love to provide real value for you, we want you to succeed!</p>
                    </div>
                </div>
                <!-- col-md-4 end here -->
                <div class="col-md-4">
                    <!-- col-md-4 start here -->
                    <div class="service-panel-heading">
                        <i class="l-basic-gear color-red"></i>
                        <h4>Flexible options</h4>
                    </div>
                    <div class="service-panel-content">
                        <p>Let us know how we can serve you better, our commitment to make a difference for our customers is complete!</p>
                    </div>
                </div>
                <!-- col-md-4 end here -->
                
            </div>
            <!-- / .row -->
        </div>
    </section>
    <!-- / SECTION END -->
    <!-- SECTION START -->
    <section class="services-preview gray-lighter-bg">
        <div class="container">
            <!-- .container -->
            <div class="row">
                <!-- .row start -->
                <div class="heading mt30 mb30 text-center">
                    <h3>Pricing</h3>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-6">
                    <!-- col-lg-3 start here -->
                    <div class="pricing-table">
                        <div class="pricing-header">
                            <h3>Trial</h3>
                        </div>
                        <ul class="list-group">
                            
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Customer Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Competitive Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Market Segment Report</li>
                            <li class="list-group-item "><i class="glyphicon glyphicon-share s24"></i> Unlimited Report Sharing</li>
                            <li class="list-group-item "><i class="glyphicon glyphicon-user s24"></i> 1 user</li>
                            <li class="list-group-item " style="visibility:hidden"><i class="glyphicon glyphicon-globe s24"></i> Custom Reports</li>
                            
                        </ul>
                        <div class="pricing-bottom clearfix">
                            <div class="price">
                                <span>0$</span>
                                / 30 days
                                @Html.ActionLink("Start your trial", "Subscribe", "Intake", new { ProductType = "Trial" }, new { @class = "btn btn-danger btn-lg btn-block" }) 
                            </div>
                        </div>
                    </div>
                </div>
                <!-- col-lg-3 end here -->

                <div class="col-lg-4 col-md-4 col-sm-6">
                    <!-- col-lg-3 start here -->
                    <div class="pricing-table">
                        <div class="pricing-header">
                            <h3>Economic</h3>
                        </div>
                        <ul class="list-group">
                            
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Customer Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Competitive Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Market Segment Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-share s24"></i> Unlimited Report Sharing</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-user s24"></i> Up to 10 users</li>
                            
                            <li class="list-group-item" style="visibility:hidden"><i class="glyphicon glyphicon-globe s24"></i> Custom Reports</li>
                            
                        </ul>
                        <div class="pricing-bottom clearfix">
                            <div class="price">
                                <span>499$</span>
                                / month
                                @Html.ActionLink("Order Now", "Subscribe", "Intake", new { ProductType = "Economic" }, new { @class = "btn btn-danger btn-lg btn-block" })
                                
                            </div>
                        </div>
                    </div>
                </div>
                <!-- col-lg-3 end here -->
                <div class="col-lg-4 col-md-4 col-sm-6">
                    <!-- col-lg-3 start here -->
                    <div class="pricing-table">
                        <div class="pricing-header">
                            <h3>Business</h3>
                        </div>
                        <ul class="list-group">
                            
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Customer Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Competitive Report</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-signal s24"></i> A Market Segment Report</li>
                            
                            <li class="list-group-item"><i class="glyphicon glyphicon-share s24"></i> Unlimited Report Sharing</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-user s24"></i> Up to 50 company users</li>
                            <li class="list-group-item"><i class="glyphicon glyphicon-globe s24"></i><b> Customized Report</b></li>
                            
                        </ul>
                        <div class="pricing-bottom clearfix">
                            <div class="price">
                                <span>999$</span>
                                / month
                                @Html.ActionLink("Order Now", "Subscribe", "Intake", new { ProductType = "Business" }, new { @class = "btn btn-danger btn-lg btn-block" })
                            </div>
                        </div>
                    </div>
                </div>
                <!-- col-lg-3 end here -->
            </div>
            <!-- / .row -->
        </div>
        <!-- / .container -->
    </section>
    <!-- / SECTION END -->
</div>
<!-- / #content -->
<div id="overlay" class="overlay"></div>

